@using Learning.Models
@model List<File1Model>

@{
    ViewBag.Title = "Xmldetailsfile";
}

<h2>Xmldetailsfile</h2>
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <table>
        <tr>
            <th>File Name</th>
            <th></th>
        </tr>

        @foreach (File1Model file in Model)
        {
            <tr>
                <td>@file.FileName</td>
                <td>@Html.ActionLink("Download", "Downloadf", new { fileName = file.FileName })</td>
            </tr>
        }
    </table>

    <p>@Html.ActionLink("Download1", "Downloadf")</p>
</body>
</html>